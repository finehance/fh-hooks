{
  "name": "fh-hooks",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Karol Stopyra",
  "repository": {
    "type": "git",
    "url": "git@github.com:stopyransky/fh-hooks.git"
  },
  "main": "dist/fh-hooks.cjs.js",
  "module": "dist/fh-hooks.esm.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint $PWD/'**/*.[jt]s?(x)'",
    "lint:fix": "prettier-eslint $PWD/'**/*.[jt]s?(x)' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.[jt]s?(x)": [
      "npm run lint:fix"
    ]
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "typescript": "4.0.3"
  },
  "dependencies": {}
}
